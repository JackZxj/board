    <script>pipeline {
    agent any
    stages {
        stage(&apos;jenkins&apos;) {
            
            steps {
                echo &quot;Start to run .......&quot;
                sh &apos;/home/jenkins/clone.py $base_repo_url $WORKSPACE $BUILD_ID&apos;
            
            }
            
        }
    }
}
</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
