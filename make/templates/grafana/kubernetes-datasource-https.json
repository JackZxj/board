{
	"name": "board-cluster",
	"type": "grafana-kubernetes-datasource",
	"url": "$kube_http_scheme://$kube_master_ip:$kube_master_port",
	"withCredentials": false,
	"access": "proxy",
	"jsonData": {
		"ds": "Prometheus",
		"tlsAuthWithCACert": true,
		"tlsAuth": true
	},
	"secureJsonData": {
		"tlsCACert": $ca_cert,
		"tlsClientKey": $grafana_key,
		"tlsClientCert": $grafana_cert
	}
}