    "@types/core-js": "~0.9.42",
    "@types/jasmine": "^2.5.53",
    "@types/jasminewd2": "^2.0.2",
    "@types/node": "^8.0.24",
    "bootstrap": "4.0.0-alpha.5",
    "enhanced-resolve": "^3.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.2.1",
