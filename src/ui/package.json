{
  "name": "clarity-seed",
  "version": "0.9.0",
  "description": "Angular-CLI starter for a Clarity project",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve --proxy-config proxy.config.json --host 0.0.0.0",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --single-run --code-coverage",
    "showcoverage": "http-server -c-1 -o -p 9875 ./coverage",
    "aot": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "rollup": "node_modules/.bin/rollup -c src/rollup/rollup-config.js",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor protractor.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~5.0.0",
